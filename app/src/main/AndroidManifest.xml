<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.polillas.cocleapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/icon"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activities.DificultyActivity">
        </activity>
        <activity android:name=".activities.ConfigTerapistActivity">
        </activity>
        <activity android:name=".activities.TerapistActivity">
        </activity>
        <activity android:name=".activities.AccountActivity">
        </activity>
        <activity android:name=".activities.ScoreActivity">
        </activity>
        <activity android:name=".activities.ExerciseActivity">
        </activity>
        <activity android:name=".activities.PracticeActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>