<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        tools:context=".activities.TerapistActivity">

    <TextView android:id="@+id/terapista_log"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text=""/>
    <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <ListView android:id="@+id/patient_list" android:layout_width="match_parent" android:layout_height="match_parent"></ListView>
    </FrameLayout>



    <Button android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Agregar paciente"/>

    <Button android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
    android:text="Desconectar"/>

</LinearLayout>